{
  "manifest_version": 2,
  "name": "Job Hunger",
  "version": "0.1",
  "browser_action": {
    "default_icon": "images/19logo.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/19logo.png",
    "128": "images/128logo.png"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/jquery-2.2.3.min.js", "js/popup.js"]
    }
  ]
}